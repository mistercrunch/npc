#include <sys/socket.h>

#define DEFAULT_PORT_I 1      /* Change this to define the default start port to scan */
#define DEFAULT_PORT_F 10000  /* Change this to define the default end port to scan */
#define MXD 10000

char version[5]="v1.0";

/* REQUESTER */

int req(int prt,char **argv)
{

    int sockfd, numbytes;
    struct hostent *host;
    struct sockaddr_in their_addr;

    char buf[MXD];

    if ((host=gethostbyname(argv[1])) == NULL) {
     printf("ERROR: Host not found!\n");
     exit;
    }

    if ((sockfd = socket(AF_INET, SOCK_STREAM, 0)) == -1) {
     printf("- Connection refused\n");
     exit;
    }

    their_addr.sin_family = AF_INET;
    their_addr.sin_port = htons(prt);
    their_addr.sin_addr = *((struct in_addr *)host->h_addr);
    bzero(&(their_addr.sin_zero), 8);

    if (connect(sockfd, (struct sockaddr *)&their_addr, sizeof(struct sockaddr)) == -1) {
     printf("- Connection refused\n");
     exit;
    }
 
    if ((numbytes=recv(sockfd, buf, MXD, 0)) == -1) {
     printf("- Error receiving data\n");
     exit;
    }

    buf[numbytes] = '\0';

    printf("%s",buf);
    
    close(sockfd);

    return 0;
}

